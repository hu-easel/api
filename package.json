    "bcryptjs": "^2.4.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "loglevel": "^1.6.1",
    "morgan": "^1.9.0",
    "mysql2": "^1.6.1",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.38.0",
